jQuery(document).ready(function(){plugin_in_header=0;if(jQuery('body').hasClass('page-template-page-plugins')||jQuery('body').hasClass('single-theme_10web_plugins')){plugin_in_header=1;}
if(plugin_in_header==1){let now,now_plus_day,get_bundle_object;now=new Date().getTime();now_plus_day=new Date(new Date().getTime()+(24*60*60*1000)).getTime();get_bundle_object={value:'1',timestamp:now_plus_day,}
localStorage.setItem('plugin_in_header',JSON.stringify(get_bundle_object));}
if(localStorage.getItem('plugin_in_header')!=null){let now,local_get_bundle,dateString;now=new Date().getTime();local_get_bundle=JSON.parse(localStorage.getItem('plugin_in_header'));dateString=local_get_bundle.timestamp;if((dateString>now)&&local_get_bundle.value=='1'){jQuery("#top-nav .menu li.plugins a,.phone-menu-div ul.menu li.plugins a,  #menu-product li.plugins a").attr("href","/wordpress-plugins/");jQuery("#top-nav li.plugins_nav_menu").css("display","inline-block");jQuery(".phone-header-part li.plugins_nav_menu").css("display","block");jQuery("body").addClass("menu-with-plugin");}else{localStorage.removeItem('plugin_in_header');}}});